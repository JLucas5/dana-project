
data Thing{
  int atr1
  int atr2
}

component provides App requires intf.Sorter sorter, data.csv.CSVParser csvParser, intf.Operator operator{

int App:Main(AppParam params[]){

    Thing array[] = csvParser.parseDocument("data.txt", "," , Thing)


      for(int i = 0; i<array.arrayLength(); i++ ){
        array[i].atr1 = operator.operation(array[i].atr1, array[i].atr2)
      }

      sorter.sort(array)

    }
}
